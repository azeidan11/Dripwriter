{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "version": 2,
  "builds": [
    { "src": "next.config.js", "use": "@vercel/next" }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "crons": [
    {
      "path": "/api/drip/process",
      "schedule": "* * * * *"
    }
  ],
  "env": {
    "NEXT_TELEMETRY_DISABLED": "1"
  }
}